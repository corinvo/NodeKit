{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false
    },
    "test:ui": {
      "cache": false
    },
    "test:e2e": {
      "cache": false,
      "dependsOn": ["build"]
    },
    "type-check": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "storybook": {
      "cache": false,
      "persistent": true
    },
    "storybook:build": {
      "dependsOn": ["build"],
      "outputs": ["storybook-static/**"]
    },
    "lint": {
      "outputs": []
    }
  }
}
